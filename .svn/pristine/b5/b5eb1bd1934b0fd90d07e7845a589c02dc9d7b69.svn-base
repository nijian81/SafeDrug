<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/xmlWhiteLineColor"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <RelativeLayout
        android:id="@+id/scan"
        android:layout_height="wrap_content"
        android:layout_width="134dp">

        <TextView
            android:id="@+id/man"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="20dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:drawablePadding="10dp"
            android:drawableLeft="@drawable/sys"
            android:text="@string/scan"
            android:textSize="@dimen/xmlMiddleContentTextSize" />

        <View
            android:layout_below="@+id/man"
            android:layout_height="1dp"
            android:layout_width="wrap_content"
            android:background="@color/xmlGrayLineColor" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/manualAdd"
        android:layout_width="135dp"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/woman"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:drawableLeft="@drawable/sgtj"
            android:drawablePadding="10dp"
            android:layout_marginLeft="20dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/manualAdd"
            android:textSize="@dimen/xmlMiddleContentTextSize" />

        <View
            android:layout_below="@+id/woman"
            android:layout_height="1dp"
            android:layout_width="wrap_content"
            android:background="@color/xmlGrayLineColor" />
    </RelativeLayout>

</LinearLayout>

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/myAccountFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bj"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_height="56dp"
        android:layout_width="match_parent"
        android:background="#5cc4aa"
        android:id="@+id/top">

        <ImageButton
            android:layout_alignParentLeft="true"
            android:id="@+id/back_family_member_manage"
            android:layout_marginLeft="@dimen/xmlTextLayoutMarginLeft"
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:scaleType="centerInside"
            android:onClick="login"
            android:layout_centerInParent="true"
            android:background="@null"
            android:src="@drawable/fanhui" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="56dp"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="@string/familyMemberManage"
            android:textColor="@color/xmlTopTitleTextColor"
            android:textSize="25sp" />

        <ImageButton
            android:id="@+id/add_familyMember"
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:onClick="login"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:background="@null"
            android:src="@drawable/tianjia" />
    </RelativeLayout>

    <View
        android:id="@+id/line"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:layout_below="@+id/myAccount"
        android:background="@color/xmlWhiteLineColor" />

    <ListView
        android:id="@+id/familyMember"
        android:layout_below="@+id/top"
        android:layout_width="match_parent"
        android:divider="#65b5a4"
        android:dividerHeight="1dp"
        android:background="#d9ede7"
        android:layout_height="wrap_content" />

</LinearLayout>